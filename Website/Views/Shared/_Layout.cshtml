<!DOCTYPE html>
<html>
<head>
    @{
        string ttle = ViewBag.Title + " - WebsiteName";
        string url = Data.Common.Sitesettings.WebsitePath + ViewBag.SEOUrl;
    }
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <!--<script async src="https://www.googletagmanager.com/gtag/js?id=G-SJP329WSHQ"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-SJP329WSHQ');
    </script>-->
    <title>@ttle</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="content-language" content="en" />
    <meta name="keywords" content="@ViewBag.SEOkeywords" />
    <meta name="title" content="@ttle" />
    <meta name="description" content="@ViewBag.SEODescription" />
    <meta property="og:title" content="@ViewBag.Title" />
    <meta property="og:description" content="@ViewBag.SEODescription" />
    <meta property="og:url" content="@url" />
    <meta property="og:image" content="@Data.Common.Sitesettings.ShareImage" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/favicon.ico")" />
    @Styles.Render("~/Content/css")

    <link rel="apple-touch-icon" href="images/mobileicon.png" />
    <meta name="HandheldFriendly" content="True" />
    <meta name="MobileOptimized" content="1100" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    @Html.AntiForgeryToken()



    @Html.Action("_Header", "Home")

    @RenderBody()

    @Html.Action("_Footer", "Home")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Content/scripts")
    @Scripts.Render("~/bundles/validate")
    @RenderSection("scripts", required: false)
</body>
</html>
